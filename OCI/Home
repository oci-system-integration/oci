<script>

var image_track = 'a';

function swap()
{
	var image = document.getElementById('SwapPic');

	if(image_track == 'a')
	{
		image.src = 'images/AboutUs.png';
		image_track = 'b';
	}

	else
	{
		image.src = 'images/c1.png';
		image_track = 'a';
	}
}

	var timer1 = setInterval('swap()', 2000);
</script>

<img src= "c1.png" width="400" height="300" id="SwapPic">

<img src= "c1.png" width="400" height="300" id="a">
<img src= "c1.png" width="400" height="300" id="b">
